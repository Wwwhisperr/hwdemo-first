<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!--add-->
    <servlet-mapping>
        <servlet-name>add</servlet-name>
        <url-pattern>/add.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>add</servlet-name>
        <servlet-class>com.example.homework_servlet.web.addServlet</servlet-class>
    </servlet>
    <!--list-->
    <servlet-mapping>
        <servlet-name>list</servlet-name>
        <url-pattern>/list.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>list</servlet-name>
        <servlet-class>com.example.homework_servlet.web.listServlet</servlet-class>
    </servlet>

</web-app>